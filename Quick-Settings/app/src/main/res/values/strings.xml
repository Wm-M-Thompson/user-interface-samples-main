<resources>
    <string name="app_name">Quick Settings Sample</string>
    <string name="tile_label">Quick Settings Sample</string>
    <string name="app_greeting">
        <b>Manual</b>\nOpen the notification shade to
        add the sample tile. Swipe down from the top of the phone to bring down
        the notification shade and then click the <b>Edit</b> button to add the
        Quick Settings tile.
    </string>
    <string name="add_tile_for_user">
        <b>App request from user</b>\nFor API level
        33+, the app can add the tile on your behalf with consent.
        Click \'Add Quick Settings tile\' to test this.
    </string>
    <string name="add_tile_for_user_btn_label">Add Quick Settings tile</string>
    <string name="service_active">active</string>
    <string name="service_inactive">inactive</string>

    <string name="qs_dialog_tile_label">QS Dialog Launcher</string>
    <string name="qs_dialog_prompt">Change the tile state?</string>
    <string name="qs_dialog_active">Turn off</string>
    <string name="qs_dialog_inactive">Turn on</string>
    <string name="qs_dialog_cancel">Cancel</string>

    <string name="qs_intent_tile_label">QS Intent Launcher</string>

    <string name="result_label">Quick Settings Result Activity</string>
    <string name="result_output">The %1$s tile is %2$s.</string>
    <string name="result_return">Return to home</string>

</resources>
